# ========================================
# Interactive Deep Research 环境配置示例
# ========================================

# ========================================
# 数据库配置 (PostgreSQL)
# ========================================
# PostgreSQL 数据库主机
PG_HOST=localhost

# PostgreSQL 数据库端口
PG_PORT=5432

# PostgreSQL 数据库名
PG_DATABASE=langgraph_research

# PostgreSQL 用户名
PG_USER=postgres

# PostgreSQL 密码
PG_PASSWORD=your_password

# PostgreSQL SSL 模式
PG_SSLMODE=prefer

# PostgreSQL 连接URL (可选，会覆盖上面的单独配置)
PG_URL=postgresql://postgres:your_password@localhost:5432/langgraph_research?sslmode=prefer

# ========================================
# Checkpoint 配置
# ========================================
# 是否启用 PostgreSQL checkpoint (true/false)
USE_POSTGRES_CHECKPOINT=true

# ========================================
# Redis 配置
# ========================================
REDIS_URL=redis://localhost:6379

# ========================================
# API 配置
# ========================================
# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Tavily API Key (用于网络搜索)
TAVILY_API_KEY=your_tavily_api_key

# ========================================
# 应用配置
# ========================================
# 应用环境 (development/production)
APP_ENV=development

# 调试模式
DEBUG=true

# 日志级别
LOG_LEVEL=INFO
